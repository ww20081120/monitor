#foreach($cscope in $scope)
     <div style="width: 1000%;overflow: auto;">
        <div class="col-sm-3" style="margin-left:-5px;mamargin-top:3PX;width: 600%;">
                <div class="ibox-title">
                    <h5>$floor.get($cscope) $!{cscope}</h5>
                </div>
        </div>

        #foreach($cname in $name.get($cscope))
        <div class="col-sm-3" style="margin-left: -5px; margin-top: 10px;width: 450px;">
                <div class="ibox">
                    <div class="ibox-title">
                        <span class="label label-primary pull-right"></span>
                        <h5>$floor.get($cscope) $!{cname}</h5>
                    </div>
                    <div class="ibox-content">
                        <table class="table table-bordered">
                            <thead>
                            <tr>
                                <th>层</th>
                                <th>ip</th>
                                <th>其他</th>
                                <th>状态</th>
                            </tr>
                            </thead>
                            <tbody>
                            #foreach($data in $cabinetData.get($cname))
                            <tr>
                                <td>$!{data.cabinetLevel}</td>
                                <td><span onclick="showDetail($data.serverId)">IP:$!{data.ipAddress}<br>型号:$!{data.openPort}</span></td>
                                <td>
                                #if($data.managerIp)
                                 管理卡:&nbsp;<a  target="_blank" href="https://$!{data.managerIp}">$!{data.managerIp}</a><br>
                                    #else
                                    <br>
                                #end
                                #if($data.switchId)
                                交换机:&nbsp;$!{data.switchId}<br>
                                    #else
                                    <br>
                                #end
                                #if($data.switchPort)
                                端口:&nbsp;$!{data.switchPort}
                                    #else
                                    <br>
                                #end
                            </td>

                                <td title="红色为ip地址不可达,灰色为没有配置设备,绿色为设备IP可达">
                                    <button class="btn #if(  ${data.status}==1 ) btn-primary #end #if(${data.serverId}>=0 && ${data.status}==0 )
btn-danger #end btn-circle circle" type="button">
                                    </button>
                                </td>
                            </tr>
                            #end
                            </tbody>
                        </table>
                    </div>
                </DIV>
            </div>
            #end
        </div>
     </div>
#end

</div>
