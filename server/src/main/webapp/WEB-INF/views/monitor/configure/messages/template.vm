        border: 1px solid #ddd;
    }
    .table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th {
        padding: 8px;
        line-height: 1.42857143;
        vertical-align: top;
        border-top: 1px solid #ddd;
    }
    td, th {
        padding: 0;
    }

    .ibox-title {
        border-top:5px solid #1ab394;
        width: 100%;
        height: 60px;;
    }
    .ibox-title h6 {
        display: inline-block;
        font-size: 20px;
        margin: 0 0 7px;
        padding: 0;
        text-overflow: ellipsis;
        float: left;
        margin-top: 15px;
        margin-left: 30px;
        margin-bottom: 5px;;
    }

    .ibox-title .label {
        float: left;
        margin-left: 4px;
    }
    .padding {
        padding-bottom: 15px;
        margin-top: 10px;;
        margin-left: 20px;
        color: bl;
    }

</style>
<div class="col-sm-6">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h6>${SUBJECT}</h6>
        </div>
        <div class="ibox-content">

            <table class="table">
                <tbody>
